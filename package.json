{
  "name": "js-utils-pack",
  "version": "0.0.1",
  "description": "A collection of js utility.",
  "main": "dist/index.js",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties"
      ]
    ]
  },
  "scripts": {
